{
	'types': [
		{
			'name': 'integer',
			'class': 'C3',
			'rule': /\d/
		},
		{
			'name': 'float',
			'class': 'C6',
			'rule': /\d/
		},
		{
			'name': 'identifier',
			'class': 'C1',
			'rule': /\w/
		},
		{
			'name': 'reservedWord',
			'class': 'C2',
			'rule': /program|begin|end|var|real|integer|procedure|read|write|while|do|if|else|then/
		},
		{
			'name': 'specialCharacter',
			'class': 'C4',
			'rule': /<>|:=|<=|>=|\(|\)|\*|\/|-|\+|=|>|<|\$|\:|;|\,/
		},
		{
			'name': 'comments',
			'class': 'C5',
			'rule': /\{\}/
		}
	],
	'scopeDelimiters': [
		{
			'startDelimiter': /^program/,
			'stopDelimiter': /.$/,
			'scope': 'global'
		},
		{
			'startDelimiter': /begin/,
			'stopDelimiter': /end/,
			'scope': 'block'
		},
		{
			'startDelimiter': /procedure/,
			//TODO: change procedure end delimiter?
			'stopDelimiter': /end/,
			'scope': 'procedure'
		}
	],
	//TODO: implement errors
	'errors': [
		{
			'code': '001',
			'message': '',
			'scope': 'lexical'
		}
	]
}
